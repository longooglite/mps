#wf-workflow-loading{margin-left:39px;}
.wf-item-optional{float:right; color:#aaa; font-size:.9em; font-style:italic;}
.wf-qa-optional{float:left; margin-right:10px; color:#aaa; font-size:1em; font-style:italic; font-weight:normal;}
.wf-form-right .wf-qa-optional{float:none; position:absolute; left:101%; top:6px;}
.cvRowRepeating_Select .wf-qa-optional{left:auto !important; right:-110px !important}

#wf-rooster .cv-table-loading{clear:left;}

.FixedHeader_Header .wf-data-table thead .row th,
.table > thead:first-child > tr:first-child > th{border-top:solid 0px #333 !important;}
.wf-details-form{margin-bottom:15px !important;}
.wf-details-form .row{clear:left; margin-bottom:5px !important;}
.wf-person-page table tbody td{vertical-align:middle;}
#wf-details-content-body{min-height:100px;}
#wf-workflow-loading-inner{position:absolute; left: 10px; top:80px;}

/*
/  Academic Evaluator Upload Page
*/
.wf-eval-upload{padding-top:60px !important; position:absolute; top:0; bottom:0; left:0; right:0;}
.wf-eval-upload .page-header{margin:10px 0 0 0 !important; border-bottom-color:#ccc; padding-left:30px;}
.wf-eval-upload .page-header h1{font-size:1.6em !important; margin-top:10px !important;}
.wf-eval-upload .navbar{display:none;}
.wf-eval-upload .mpsfooter-navbar{display:block !important;}
.wf-eval-upload .container-fluid{position:absolute; top:60px; bottom:0px; left:0; right:0; min-height:400px;}
.wf-eval-upload .wf-visitor-content{}
.wf-eval-upload .wf-candidate-info{bottom:0;}
.wf-eval-upload .mpsheader{z-index:999;}

.wf-visitor-content{padding:20px 0 0 280px !important; position:relative; z-index:9 !important;}
.wf-eval-upload .wf-candidate-info{
    position:absolute;
    top:55px;
    left:0px;
    margin:0 0px 0 0;
    font-size:1.1em;
    background:#eee;
    border-left:solid 1px #999;
    padding:20px 20px 20px 30px;
    min-height:300px;
    width:250px;
    color:#999;
    z-index:1;
}
.wf-eval-upload .wf-candidate-info p{color:#333; padding:2px 5px 2px 15px !important; margin:0px !important;}
.wf-eval-upload .wf-candidate-info .first{margin-top:8px !important; color:#111; font-weight:bold;}
.wf-eval-upload .container-fluid{background-color:#fff;}
.wf-eval-upload .page-header{background-color:#f9f9f9; margin-top:0px !important; padding-top:10px !important;}
.wf-eval-upload .page-header h1{margin-top:5px !important;}
.heavy{font-weight:bold !important;}

/* End Visitor Upload page */

.wf-page-callout{width:50%; min-width:650px;}
.wf-page-callout-wide{width:80%; min-width:650px;}
.wf-page-callout textarea{width:100%;}
.wf-details-page .page-header{border-bottom-color:#ccc !important;}
.wf-overview-item-wrap .wf-approval-denied{color: darkred !important;}
.wf-item-after-clickable{cursor:pointer;}
.wf-uber-chkbox .wf-uber-left{padding-right:5px;}
.wf-uber-chkbox input{position:relative; top:5px; margin-right:4px;}

/* RFP Templates UXs */
.wf-templates-wrap{padding:10px; margin-bottom:20px; width:80%; display:none;}
.wf-templates-wrap .row{margin-left:5px !important; margin-right:5px !important;}
.wf-templates-wrap .row div{border-bottom:solid 1px #ddd; padding:5px !important;}
.wf-templates-close{float:right; margin-top:-2px;}
.wf-templates-close .glyphicon{margin-right:2px;}
.wf-template-delete{text-align:right}

/* READ version of Personal Info Overrides */
.wf-cv-form-override .wf-summary-values{border:0px; padding-left:20px;}
.wf-cv-form-override .wf-summary-values .cv-panel{float:left !important; width:48%; clear:none; margin-right:2%; min-width:300px;}
.wf-cv-form-override .cv-panel-heading{display:block; float:none; width:95%; top:0; margin:0; text-align:left; line-height:30px; border-bottom:solid 1px #666; position:relative !important;}
.wf-cv-form-override .cv-panel-heading .mpsbold{line-height:30px; color:#333; clear:both;}
.wf-cv-form-override .cv-panel-body{margin:0; top:0;}
.wf-cv-form-override .cv-panel-body .wf-summary-line{position:relative;}
.wf-cv-form-override .cv-panel-body .wf-summary-line .wf-summary-label{width:40%; font-weight:normal !important; padding:3px 5px;}
.wf-cv-form-override .cv-panel-body .wf-summary-line .wf-summary-value{width:55%; font-weight:bold; display:inline-block; padding:3px 5px;}
.wf-cv-form-override .cv-panel-body-textblob{padding-left:20px;}
.wf-cv-form-override .cv-panel-body-narrow-labels .wf-summary-line .wf-summary-label{width:18%;}
.wf-cv-form-override .cv-panel-body-narrow-labels .wf-summary-line .wf-summary-value{width:77%;}

/* Similar CBGC Page Read view */
.wf-cbgc-page h3{border:0px !important; background-color:#fff !important; border-bottom:solid 1px #aaa !important; font-size:1.2em;}
.wf-cbgc-summary{width:96% !important; padding-left:20px;}
.wf-cbgc-btn{margin-right:1%;}
.wf-cbgc-resubmit{margin-right:4%;}

.liteGray{opacity:.25;}
.medGray{opacity:.5;}
.wf-instructional-text{font-style:italic !important;}

/* Candidate View: New DropDown vs. LeftNav Dials */
.wf-candidate-dials{margin:10px;}

/* Export & Print Buttons */
.dt-buttons {display:none;}
.dt-buttons .dt-button{margin-right:10px;}

.wf-candidate-parent{padding:15px 10px 0 10px;}
.wf-candidate-parent a{color:#036; font-weight:bold;}
.wf-candidate-parent .wf-dial-btn{
    height: 22px !important;
    width: 21px !important;
    margin: 0px !important;
    padding:1px 6px 0 4px;
    float:right;
    color:#69c;
    position:relative;
    top:-1px;
}

.Tabled-Form-Wrapper .Repeating-Form{position:relative; top:-15px;}
.Repeating-Form-Table{margin-bottom:10px !important;}
.Tabled-Form-Wrapper .cv-remove-field{position:relative; top:0px; margin-left:0px;}
.Tabled-Form-Wrapper h4 .cv-remove-field{margin-left:30px;}
.wf-tabled-form-edit .glyphicon{margin-left:5px;}
.wf-roster-date-pick{width:100px; position:relative;}
.wf-roster-date-wrap .input-group-addon{padding:4px 8px !important; position:relative;/* top:0 left:3px;*/}
.wf-roster-date-wrap input{position:relative; right:3px;}
.wf-print-item{margin-top:0 !important;}
.wf-print-item .glyphicon{margin-right:5px;}


/* Field Level Revisions Required */
.wf-flrr-edit-form{position:relative;}
.wf-flrr-global-comment{display:block; width:83%;}
.wf-flrr-global-comment textarea{}
.wf-flrr-edit-form .wf-flrr-title{display:block;}
.wf-flrr-edit-form textarea{display:block !important; width:100%; margin:3px 0 0 0px; padding:5px;}
.wf-flrr-edit-form .wf-flrr-save-button{position:absolute; top: 24px; right:-45px;}
.wf-flrr-edit-form .wf-flrr-cancel-button{position:absolute; top: 24px; right:-90px;}
.wf-flrr-edit-form .wf-flrr-enabled{margin:0px 1px 0 0; position:relative; top:1px;}
.wf-flrr-edit-form ::-webkit-input-placeholder {
   font-style: italic;
}
.wf-flrr-edit-form :-moz-placeholder { /* Firefox 18- */
   font-style: italic;
}
.wf-flrr-edit-form ::-moz-placeholder {  /* Firefox 19+ */
   font-style: italic;
}
.wf-flrr-edit-form :-ms-input-placeholder {
   font-style: italic;
}

.wf-flrr-note, .wf-flrr-title{color:darkorange;font-style:italic; margin-top:2px; background-color:#fff;}
.wf-flrr-note .wf-flrr-label{font-style:normal;}

.wf-flrr-item-button{position:absolute; top:0px; right:-1px; z-index:2;}
.form-row .wf-form-right .wf-flrr-border-on, .wf-flrr-border-on{padding-left: 5px !important; border:solid 1px #ccc; border-radius:3px; line-height:19px !important;}
.wf-item-is-disabled .wf-flrr-border-on{background-color:#eee;}
.wf-flrr-global-header{margin-top:20px !important;}
.wf-flrr-global-header .wf-flrr-global-note, .wf-flrr-global-header .wf-item-before{color:darkorange !important;}

.wf-flrr-repeater-checkbox{border-radius:8px; border: solid 1px red; padding:10px; clear:both; width:200px; color:darkorange; font-style:italic; float:left;}

.tbl-sum-flrr{color:darkorange;}
.tbl-row-flrr{position:absolute; top:7px; left:-22px;}

